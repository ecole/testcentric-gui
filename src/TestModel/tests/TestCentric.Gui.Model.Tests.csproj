        </ProjectReference>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="NSubstitute" Version="4.0.0" />
        <PackageReference Include="NUnit" Version="3.13.2" />
        <PackageReference Include="NUnit.Engine.Api" Version="4.0.0-dev-05363" />
